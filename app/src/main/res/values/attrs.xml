<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RippleBackground">
        <attr name="rb_color" format="color" />
        <attr name="rb_strokeWidth" format="dimension"/>
        <attr name="rb_radius" format="dimension"/>
        <attr name="rb_duration" format="integer"/>
        <attr name="rb_rippleAmount" format="integer"/>
        <attr name="rb_scale" format="float"/>
        <attr name="rb_type" format="enum">
            <enum name="fillRipple" value="0"/>
            <enum name="strokeRipple" value="1"/>
        </attr>
    </declare-styleable>
    <attr name="primaryColor" format="reference" />
    <attr name="primaryDarkColor" format="reference" />
    <attr name="primaryColorAccent" format="reference" />
    <attr name="primaryTextColor" format="reference" />
    <attr name="secondaryTextColor" format="reference" />
    <attr name="dividerColor" format="reference" />
    <attr name="roundedCornersButton"  format="reference"/>
    <attr name="homeScreenBtnBackground" format="reference"/>
    <attr name="svgStrokeColor" format="reference"/>
    <attr name="svgFillColor" format="reference"/>
    <attr name="accentGood" format="reference"/>
    <attr name="accentBad" format="reference"/>
    <attr name="backgroundDark" format="reference"/>

</resources>
